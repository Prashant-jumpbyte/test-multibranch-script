pipeline {
  agent any
        tools {nodejs "NodeJS"}
  stages {
    stage('Init Project') {
      steps {
      sh 'npm --version'
        echo "NPM INIT"
      }
    }
       
    stage('Install dependencies') {
      steps {
        sh 'npm install'
      }
    }
     
    stage('Test') {
      steps {
         sh 'npm test'
      }
    }      
  }
}
