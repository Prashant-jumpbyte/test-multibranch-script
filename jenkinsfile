pipeline {
    agent any

    stages {
        stage('Pull') {
            steps {
                echo 'Building..'
                sh  'ls -al'
                sh `. ~/.nvm/nvm.sh
. ~/.profile
. ~/.bashrc
if [ ! -d ~/.nvm ]; then
  curl https://raw.githubusercontent.com/creationix/nvm/v0.11.1/install.sh | bash
  source ~/.nvm/nvm.sh
  source ~/.profile
  source ~/.bashrc
  nvm install 5.0
  npm install
  npm run front
  nvm install 6.9.1
  nvm list
  nvm use 6.9.1
  nvm alias default 6.9.1
fi
  nvm install 6.9.1
  nvm list
  nvm use 6.9.1
  nvm alias default 6.9.1
  node -v`
            }
        }
        stage('Build') {
            steps {
                
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
