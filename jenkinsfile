pipeline {
  agent any
        
  stages {
    stage('Init Project') {
      steps {
        sh 'npm init'
      }
    }
       
    stage('Install dependencies') {
      steps {
        sh 'npm install'
      }
    }
     
    stage('Test') {
      steps {
         sh 'npm test'
      }
    }      
  }
}
